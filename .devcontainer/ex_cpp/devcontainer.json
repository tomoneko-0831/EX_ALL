{
    "name": "ex_cpp",
    "dockerComposeFile":"./../../docker-compose.yml",
    "service": "ex_cpp",
    "runServices": [
        "ex_cpp"
    ],
    "profiles":{
        "ex_c": {
            "service": "ex_c"
        },
        "ex_cpp": {
            "service": "ex_c_cpp"
        },
        "ex_java": {
            "service": "ex_java"
        },
        "ex_js": {
            "service": "ex_js"
        },
        "ex_py": {
            "service": "ex_py"
        },
        "dev_win": {
            "service": "dev_win"
        }
    },
    "workspaceFolder": "/Cpp",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools-extension-pack",
                "oderwat.indent-rainbow",
                "ms-vscode-remote.remote-containers",
                "ms-azuretools.vscode-docker"
            ]
        }
    },
    "shutdownAction": "stopCompose"
}
